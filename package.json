{
  "name": "search-page-react-next",
  "private": true,
  "engines": {
    "node": ">=10",
    "pnpm": ">=3"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "scripts": {
    "postinstall": "$CI = true || editor-config",
    "prepare": "husky",
    "start": "concurrently \"react-scripts start\" \"node server.js\"",
    "server": "node server.js",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "checks": "pnpm format:check && pnpm typecheck && pnpm lint",
    "format:check": "prettier --check --ignore-unknown \"**/*\"",
    "format": "npm run format:check -- --write",
    "typecheck": "tsc --project tsconfig.json",
    "lint": "eslint --ext js,jsx,ts,tsx ./src --max-warnings 0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.9",
    "@mui/material": "^5.15.9",
    "express": "^4.18.2",
    "fuse.js": "^6.6.2",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@tiagoporto/editor-config": "^2.1.0",
    "@tiagoporto/prettier-config": "^2.1.0",
    "@types/express": "^4.17.17",
    "@types/node": "^16.18.53",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001"
}
